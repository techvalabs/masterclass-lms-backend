{
  "name": "masterclass-lms-backend",
  "version": "1.0.0",
  "description": "Real Estate Masterclass LMS - Backend API Server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=2048 tsx watch src/server.ts",
    "dev:windows": "set NODE_OPTIONS=--max-old-space-size=2048 && tsx watch src/server.ts",
    "dev:standard": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node --max-old-space-size=2048 dist/server.js",
    "start:standard": "node dist/server.js",
    "db:create": "mysql -u root -p -e \"CREATE DATABASE IF NOT EXISTS masterclass_lms CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;\"",
    "db:schema": "mysql -u root -p masterclass_lms < database/schema.sql",
    "db:seed": "tsx src/database/seeders/index.ts",
    "db:reset": "npm run db:create && npm run db:schema && npm run db:seed",
    "admin:create": "tsx scripts/create-admin.ts",
    "admin:fix": "tsx scripts/fix-admin.ts",
    "migrate": "node database/utilities/migration-runner.js migrate",
    "migrate:status": "node database/utilities/migration-runner.js status",
    "migrate:seed": "node database/utilities/migration-runner.js seed",
    "migrate:reset": "node database/utilities/migration-runner.js reset",
    "migrate:validate": "node database/utilities/migration-runner.js validate",
    "import:users": "node database/utilities/data-importer.js import-users",
    "import:courses": "node database/utilities/data-importer.js import-courses",
    "import:sample": "node database/utilities/data-importer.js sample",
    "export:data": "node database/utilities/data-importer.js export",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "lms",
    "real-estate",
    "education",
    "nodejs",
    "express",
    "mysql",
    "typescript"
  ],
  "author": "Real Estate Masterclass Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.864.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/handlebars": "^4.1.0",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.15",
    "@types/sharp": "^0.32.0",
    "@types/socket.io": "^3.0.1",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.4.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.1",
    "get-video-duration": "^4.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.14",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
